<local:MyCard Margin="0,0,0,12">
    <StackPanel Margin="24,12,24,10">
        <Grid Margin="0,8,0,8">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="80" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Line X1="0" X2="100" Stroke="{DynamicResource ColorBrush3}" StrokeThickness="1.5" Stretch="Fill" Grid.Column="0" />
            <TextBlock Text="网页重引导" FontSize="18" Foreground="{DynamicResource ColorBrush4}" Grid.Column="1" Margin="0,0,0,4" VerticalAlignment="Center" HorizontalAlignment="Center" />
            <Line X1="0" X2="100" Stroke="{DynamicResource ColorBrush3}" StrokeThickness="1.5" Stretch="Fill" Grid.Column="2" />
        </Grid>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <local:MyTextBox HintText="在此输入网页链接…" Margin="0,0,0,10" Name="site" Height="30" Grid.Column="0" ToolTip="在此输入链接，点击右边按钮打开自定义网页。&#xA;注意不要按下 Enter 键，这可能会直接启动游戏。" Text="https://jh6485622.github.io" />
            <local:MyButton EventType="打开网页" EventData="{Binding ElementName=site,Path=Text}" Text="打开网页" Margin="5,0,0,10" Height="30" Grid.Column="1" ToolTip="点我打开" Padding="2,0,2,0" ColorType="Highlight" />
        </Grid>
        <local:MyListItem Margin="0,0,2,0" Logo="pack://application:,,,/images/blocks/Grass.png" Title="刷新一下" EventType="刷新主页" Type="Clickable" />
        <local:MyListItem Margin="0,0,2,0" Logo="pack://application:,,,/images/blocks/GoldBlock.png" Title="关于本页" EventType="弹出窗口" EventData="关于本页|©jh6485622 2026/2/22\n\n此页面是泡菜龙大杂烩的子页面。\n\n本页收集了亿点网站，如有不全，欢迎前往github.com/jh6485622/jh6485622.github.io去砸Issue(有时候PR也行，我不是很爱上线)。" Type="Clickable" />
    </StackPanel>
</local:MyCard>
